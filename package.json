  },
  "bugs": {
    "url": "https://github.com/mdnorman/CHANGEME/issues"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf out",
    "copy-files": "mkdir -p dist && copyfiles --up 1 'src/**/*.model.json' dist",
    "build": "npm run copy-files && rbxtsc",
    "build-watch": "npm run copy-files && rbxtsc -w",
    "build:development": "npm run copy-files && rbxtsc",
    "rebuild": "npm run clean && npm run build",
    "pretest": "npm run build && tsc -p spec/tsconfig.json",
    "rojos": "rojo serve",
    "test": "jasmine",
    "posttest": "npm run lint",
    "lint": "eslint",
    "pretty": "pretty-quick"
  },
  "devDependencies": {
    "@rbxts/types": "1.0.553",
    "@types/jest": "27.4.0",
    "@types/node": "16.11.19",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "copyfiles": "2.4.1",
    "eslint": "8.6.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.4.7",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "roblox-ts": "1.2.7",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "tsutils": "3.21.0",
    "typescript": "4.5.4"
  },
  "engines": {
    "npm": ">=7.20",
    "node": ">=16.5"
  }
}
